<?xml version="1.0"?>
<robot name="first_robot">
    <material name="my_color">
        <color rgba="0 0 1 1"/>
    </material>
        <material name="my_color2">
        <color rgba="1 0 1 1"/>
    </material>
        <material name="my_color3">
        <color rgba="0 1 1 1"/>
    </material>
<link name="base_footprint">
</link>
<joint name="base_joint" type="fixed">
<parent link="base_footprint"/>
<child link="base_link"/>
<origin xyz="0 0 0.1" rpy="0 0 0"/>
</joint>
<link name="base_link">
        <visual>
        <geometry>
            <box size="0.6 0.4 0.2"/>
        </geometry>
        <material name="my_color" />
    <origin xyz="0 0 0.1" rpy="0 0 0"/>
    </visual>
    <collision>
            <geometry>
                <box size="0.6 0.4 0.2" />
            </geometry>
            <origin xyz="0 0 0.1" rpy="0 0 0" />
            </collision>
    <inertial>
            <origin xyz="0 0 0.1" rpy="0 0 0" />
            <mass value="5" />
            <inertia ixx="0.083" ixy="0" ixz="0"
                iyy="0.167" iyz="0"
                izz="0.217" />
        </inertial>
</link>
<link name="cylinder_link">
    <visual>
        <geometry>
            <cylinder radius="0.1" length="0.05"/>
        </geometry>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <material name="my_color2"/>
    </visual>
    <collision>
            <geometry>
                <cylinder radius="0.1" length="0.05"/>
            </geometry>
            <origin xyz="0 0 0" rpy="0 0 0" />
            </collision>    
    <inertial>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <mass value="1" />
            <inertia ixx="0.00271" ixy="0" ixz="0" 
                iyy="0.00271" iyz="0"
                izz="0.005" />
        </inertial>
</link>
<joint name="base_cylinder_joint" type="continuous">
<parent link="base_link"/>
<child link="cylinder_link"/>
<origin xyz="-0.2 0.225 0" rpy="1.5709 0 0"/>
<axis xyz="0 0 1"/>
</joint>
<link name="cylinder2_link">
    <visual>
        <geometry>
            <cylinder radius="0.1" length="0.05"/>
        </geometry>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <material name="my_color2"/>
    </visual>
    <collision>
            <geometry>
                <cylinder radius="0.1" length="0.05"/>
            </geometry>
            <origin xyz="0 0 0" rpy="0 0 0" />
            </collision> 
    <inertial>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <mass value="1" />
            <inertia ixx="0.00271" ixy="0" ixz="0" 
                iyy="0.00271" iyz="0"
                izz="0.005" />
        </inertial>
</link>
<joint name="base_cylinder2_joint" type="continuous">
<parent link="base_link"/>
<child link="cylinder2_link"/>
<origin xyz="-0.2 -0.225 0" rpy="1.5709 0 0"/>
<axis xyz="0 0 1"/>
</joint>
<link name="sphere_link">
    <visual>
        <geometry>
            <sphere radius="0.05"/>
        </geometry>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <material name="my_color3"/>
    </visual>
    <collision>
            <geometry>
                <sphere radius="0.05"/>
            </geometry>
            <origin xyz="0 0 0.0" rpy="0 0 0" />
            </collision> 
    <inertial>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <mass value="0.5" />
            <inertia ixx="0.0005" ixy="0" ixz="0" 
                iyy="0.0005" iyz="0"
                izz="0.0005" />
        </inertial>
</link>
<joint name="base_sphere_joint" type="fixed">
<parent link="base_link"/>
<child link="sphere_link"/>
<origin xyz="0.2 0 -0.05" rpy="0 0 0"/>
</joint>

</robot>
